# platform :ios, '9.0'

target 'swift-realm-sandbox' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for scrollableGraphView-sandbox
  pod 'SnapKit', '~> 0.15.0'
  pod 'RealmSwift'

  target 'swift-realm-sandboxTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SnapKit', '~> 0.15.0'
    pod 'RealmSwift'
  end

  target 'swift-realm-sandboxUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SnapKit', '~> 0.15.0'
    pod 'RealmSwift'
  end

end
